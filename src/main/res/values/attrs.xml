<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NewDetailListItemView">
        <attr name="titleText" format="string" localization="suggested" />
        <attr name="valueText" format="string" localization="suggested" />
        <attr name="descriptionText" format="string" localization="suggested" />
        <attr name="notShownExpression" format="string" localization="suggested" />
    </declare-styleable>
    <declare-styleable name="DetailItemViewStyle">
        <attr name="detailItemViewStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="DetailListItemViewStyle">
        <attr name="detailListItemViewStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="ExpandableMathStatisticsView">
        <attr name="title" format="string" localization="suggested" />
        <attr name="type" format="string" />
        <attr name="statistics" format="reference" />
    </declare-styleable>
    <declare-styleable name="FloatingActionButton">
        <attr name="buttonPosition">
            <flag name="top" value="1"/>
            <flag name="bottom" value="2"/>
            <flag name="start" value="4"/>
            <flag name="end" value="8"/>
            <flag name="left" value="16"/>
            <flag name="right" value="32"/>
        </attr>
        <attr name="buttonIcon" format="reference"/>
        <attr name="buttonBackgroundColour" format="color"/>
        <attr name="buttonCoverColour" format="color"/>
    </declare-styleable>
</resources>